local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Event Fashion Klossette Designer Showcase", "DarkTheme")
local Tab = Window:NewTab("Event")
local Section = Tab:NewSection("RobeatDay1")
Section:NewButton("Step1", "Click him", function()
    local TeleportService = game:GetService("TeleportService")

TeleportService:Teleport(698448212, player)

end)
Section:NewButton("Step2","Click him", function()
game:GetService("ReplicatedStorage").KKShared.lib.Network.TRAIN_ENTER:FireServer()
wait (1)
local TweenService = game:GetService("TweenService")
local Tw = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(0, Enum.EasingStyle.Linear, Enum.EasingDirection.Out,0,false,0),
{CFrame = CFrame.new(1384.57092, 25.5186386, -868.685852, -0.999916434, 5.85378013e-09, -0.012928105, 6.69276812e-09, 1, -6.48531895e-08, 0.012928105, -6.49342979e-08, -0.999916434)}):Play()

end)

